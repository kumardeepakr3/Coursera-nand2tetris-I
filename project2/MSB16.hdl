/**
 * MSB - Most Significant Bit: 
 * out :  out = 1 if (in<0) else out=0 
 */

CHIP MSB16 {
    IN in[16];
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=in[15], b=false, out=out);
}